<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="2.0dip" />
            <gradient android:startColor="#ff666666" android:endColor="#ff666666" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff666666" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip android:gravity="bottom" android:clipOrientation="vertical">
            <shape>
                <corners android:radius="2.0dip" />
                <gradient android:startColor="#ffededed" android:endColor="#ffededed" android:angle="270.0" android:centerY="0.75" android:centerColor="#ffededed" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip android:gravity="bottom" android:clipOrientation="vertical">
            <shape>
                <corners android:radius="2.0dip" />
                <gradient android:startColor="#fff71c95" android:endColor="#fff71c95" android:angle="270.0" android:centerY="0.75" android:centerColor="#fff71c95" />
            </shape>
        </clip>
    </item>
</layer-list>