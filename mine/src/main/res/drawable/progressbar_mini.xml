<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient android:startColor="#33ffffff" android:endColor="#33ffffff" android:angle="270.0" android:centerX="50.75" android:centerY="50.75" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <gradient android:startColor="#4cffffff" android:endColor="#4cffffff" android:angle="270.0" android:centerX="50.75" android:centerY="50.75" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#4cffffff" android:endColor="#4cffffff" android:angle="270.0" android:centerX="50.75" android:centerY="50.75" />
            </shape>
        </clip>
    </item>
</layer-list>