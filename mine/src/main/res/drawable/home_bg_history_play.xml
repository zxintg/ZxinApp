<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
    <item android:top="-1.0px" android:right="-1.0px" android:bottom="-1.0px">
        <shape>
            <solid android:color="@android:color/transparent" />
            <stroke android:width="1.0px" android:color="#ffdcdcdc" />
        </shape>
    </item>
</layer-list>