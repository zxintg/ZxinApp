<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/dialog_sircle" />
            <solid android:color="@color/white" />
            <stroke android:width="1.0px" android:color="@color/dialog_sircle_line_color" />
        </shape>
    </item>
    <item android:state_selected="true">
        <shape>
            <corners android:radius="@dimen/dialog_sircle" />
            <solid android:color="@color/white" />
            <stroke android:width="1.0px" android:color="@color/dialog_sircle_line_color" />
        </shape>
    </item>
    <item android:state_pressed="false" android:drawable="@drawable/empty" />
</selector>