<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="0.0dip" />
            <gradient android:startColor="#009d9e9d" android:endColor="#00747674" android:angle="90.0" android:centerX="0.75" android:centerColor="#005a5d5a" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip android:gravity="bottom" android:clipOrientation="vertical">
            <shape>
                <corners android:radius="0.0dip" />
                <gradient android:startColor="#8000b4ff" android:endColor="#8000b4ff" android:angle="90.0" android:centerX="0.75" android:centerColor="#8000b4ff" />
            </shape>
        </clip>
    </item>
</layer-list>